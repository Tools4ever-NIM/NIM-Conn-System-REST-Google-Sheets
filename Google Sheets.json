{
    "schema": {
        "crud_objects": {
            "sheet_data": {
                "operations": {
                    "sheet_data_get": {
                        "method": "get",
                        "call": {
                            "mode": "normal",
                            "path": "sheets.googleapis.com/v4/spreadsheets/{sheet_id}/values/{sheet_name}"
                        },
                        "authOptions": {
                            "scopes": [
                              "https://www.googleapis.com/auth/spreadsheets"
                            ]
                        },
                        "processing_options": {
                            "output_field": "values"
                        }
                    }
                },
                "key": "id",
                "resources": { 
                    "range": "string*",
                    "majorDimension": "string*",
                    "values": ["string*"]
                }
            }
        }
    },
    "rest": {
        "config": {
            "baseUrl": "https://",
            "get": {
                "maxPageCount": -1,
                "query_parameters": {
                }
            },
            "authentication": "certificate",
            "call_handling": "google",
            "test_connection": {
                "url": "sheets.googleapis.com/v4/spreadsheets/{sheet_id}"
            }
        },
        "authOptions": {
            "scopes": [
                "https://www.googleapis.com/auth/spreadsheets"
            ],
            "type": "signed-jwt",
            "authUrl": "https://www.googleapis.com/oauth2/v4/token",
            "postData": {
                "grant_type": "urn:ietf:params:oauth:grant-type:jwt-bearer",
                "assertion": {
                    "name": "JWT"
                }
            },
            "allowedHosts": [
                    "licensing.googleapis.com",
                    "www.googleapis.com",
                    "classroom.googleapis.com",
                    "admin.googleapis.com"
                ],
           "JWT": {
                "iss": {
                    "name": "clientId"
                },
                "scope": {
                    "name": "scope"
                },
                "aud": {
                    "name": "endpoint"
                },
                "exp": {
                    "name": "oneHour"
                },
                "iat": {
                    "name": "now"
                },
                "sub": {
                    "name": "tenantId"
                }
            }
        }
    },
    "connection": {
        "items": [
            {
                "name": "sheet_id",
                "type": "textbox",
                "label": "Sheet Id:",
                "value": ""
            },
            {
                "name": "sheet_name",
                "type": "textbox",
                "label": "Sheet Range:",
                "value": "Sheet1"
            }
        ]
    }
}
